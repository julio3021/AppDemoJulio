@model AppDemoJulio.Controllers.Contacto
<h1>Contacto</h1>

<form asp-controller="Contacto" asp-action="Registrar" method="post">
    <div class="form-group">
        <label asp-for="Nombre" >Nombre:</label>
        <input asp-for="Nombre" class="form-control" />
    </div>
    <div class="form-group"></div>
    <div class="form-group">
        <label asp-for="Edad" >Edad:</label>
        <input asp-for="Edad" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Email">Email:</label>
        <input asp-for="Email" class="form-control"/>
    </div
    <div class="form-group">
        <label asp-for="Mensaje" >Mensaje:</label>
        <textarea asp-for="Mensaje" class="form-control"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" >Enviar </button>
</form>
<h1>
    @ViewData["Respuesta"]

</h1>